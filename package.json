    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "webpack": "^1.12.8",
    "webpack-dev-server": "^1.12.1"
  }
