{
  "name": "gameofthrones",
  "version": "1.0.0",
  "description": "Fantasy sports for Game of Thrones",
  "main": "./client/index.js",
  "scripts": {
    "test": "eslint **",
    "start": "NODE_ENV=development nodemon --exec babel-node server/server.js",
    "dev": "npm run submodule && webpack-dev-server --port 4000 --inline --devtool eval --progress --colors --hot --content-base dist/",
    "db": "NODE_ENV=development && npm run submodule && nodemon --exec babel-node db/server/server.js",
    "watch": "webpack --watch --colors --progress",
    "submodule": "git submodule init && git submodule update --remote"
  },
  "author": "Naomi Jacobs",
  "license": "ISC",
  "babel": {
    "presets": [
      "react",
      "stage-0",
      "es2015"
    ]
  },
  "dependencies": {
    "babel-eslint": "^4.1.6",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.1.1",
    "body-parser": "^1.14.2",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "jwt-simple": "^0.4.0",
    "mysql": "^2.10.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2",
    "request-promise": "^2.0.0",
    "sequelize": "^3.17.1",
    "socket.io": "^1.4.4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
